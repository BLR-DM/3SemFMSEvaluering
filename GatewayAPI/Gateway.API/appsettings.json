{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "test-route": {
        "ClusterId": "test-cluster",
        "Match": {
          "Path": "/test/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "test2-route": {
        "ClusterId": "test2-cluster",
        "Match": {
          "Path": "/test2/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "test-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://testservice.api:8080"
          }
        }
      },
      "test2-cluster": {
        "Destinations": {
          "destination1": {
            "Address":  "http://test2service.api:8080"
          }
        }
      }
    }
  }
}
