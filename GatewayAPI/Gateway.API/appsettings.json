{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "evaluation-route": {
        "ClusterId": "evaluation-cluster",
        "Match": {
          "Path": "/evaluation/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "userservice-route": {
        "ClusterId": "userservice-cluster",
        "Match": {
          "Path": "/userservice/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "evaluation-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://fmsevalueringapi.api:8080"
          }
        }
      },
      "userservice-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://fmsuserservice.api:8080"
          }
        }
      }
    }
  }
}
